		"@typescript-eslint/parser": "^7.0.0",
		"dotenv": "^16.4.5",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.75.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
